(function(e,a){({domA:".J-fade-in-up .J-fade-in-up-backwards .J-fade-in-special .J-width-grow .J-num-scroll .J-num-scroll-small".split(" "),_init:function(){sv.animationCollect(this.domA);this._event();this._footerShow()},_event:function(){var b=this;a(".J-contactUS-btn-m").on("click",function(){a(".J-contact-us-m").removeClass("fade-in-left").addClass("fade-in-right");a(".J-contact-us-overlay").show();a(".J-contact-us-m").show();a(".J-validate-code").hide();a(".J-header").hide()});a(".J-back-import-export,.J-cancle").on("click",
function(){a(".J-contact-us-m").removeClass("fade-in-right").addClass("fade-in-left");a(".J-contact-us-m").animate({transform:"transform 3s ease out"},500,function(){a(".J-contact-us-m").hide();a(".J-header").show();a(".J-contact-us-overlay").hide()})});a(".J-contact-us-m").on("touchstart",function(a){b.startX=a.originalEvent.changedTouches[0].pageX;b.startY=a.originalEvent.changedTouches[0].pageY});a(".J-contact-us-m").on("touchmove",function(c){var d=c.originalEvent.changedTouches[0].pageX-b.startX;
Math.abs(d)>Math.abs(c.originalEvent.changedTouches[0].pageY-b.startY)&&0<d&&(a(".J-back-import-export").trigger("click"),b.startX=0,b.startY=0)});a(".J-contact-us-m").scroll(function(){sv.animationCollect(b.domA)});a(window).scroll(function(){sv.animationCollect(b.domA)})},_footerShow:function(){0!=a(".J-consulation").length&&a(".J-footer-promotion").css({margin:"0 auto 100px"})}})._init()})(window,jQuery);